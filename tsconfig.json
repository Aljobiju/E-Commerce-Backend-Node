{
    "compilerOptions": {
      "target": "es2017",
      "module": "commonjs",  // Change to "commonjs" instead of "NodeNext"
      "outDir": "./dist",
      "strict": true,
      "esModuleInterop": true,
      "moduleResolution": "node",
      "allowImportingTsExtensions": true,
      "noEmit": true,
    },
    "include": ["src/**/*.ts","types/**/*.ts"],
    "exclude": ["node_modules", "dist"]
  }
  